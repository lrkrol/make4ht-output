@article{yeung2004passiveactivefrn,
    title = "{ERP} Correlates of Feedback and Reward Processing in the Presence and Absence of Response Choice",
    author = {Yeung, Nick and Holroyd, Clay B. and Cohen, Jonathan D.},
    year = {2004},
    journal = {Cerebral Cortex},
    volume = {15},
    number = {5},
    pages = {535--544},
    issn = {1047-3211},
    doi = {10.1093/cercor/bhh153},
    summary = {
        does FRN relate to valence in general, or only to valence as result of explicit actions? (to learn from those actions?)
        experiment 1: a random "roulette" style decision was made for them picking one out of four options for a reward after they initiated the "roulette", i.e., they initiated the spin but no active choice was made.
        result: FRN observed also in cases where no active choice was made.
        experiment 2: as above, except all actions were performed automatically (participants did not even initiate the spin).
        result: FRN osberved also in cases where no overt actions were performed at all.
        FRN amplitude in these two cases was smaller than in a case where participants did make active choices (actively choose one out of four options for a reward).
        FRN sensitive to valence, not only as a result of actions.
        FRN amplitude appears to sensitive to motivational involvement.}
}

