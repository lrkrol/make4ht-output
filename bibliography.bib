@article{holroyd2002errorbasis,
    title={The neural basis of human error processing: reinforcement learning, dopamine, and the error-related negativity},
    author={Holroyd, Clay B. and Coles, Michael G. H.},
    journal={Psychological Review},
    volume={109},
    number={4},
    pages={679--708},
    year={2002},
    doi = {10.1037/0033-295X.109.4.679},
    summary = {
        a generic error-processing system has been postulated after observing similar negative deflection in the EEG following errors on a variety of different tasks. this ERN appears to be generated in the ACC.
        separate research argues that the mesencephalic (part of the brain stem) dopamine system conveys reinforcement learning signals to the basal ganglia and frontal cortex where they are used to facilitate development of adaptive motor programs.
        both systems appear to be related to evaluating the correctness of actions and learning from potential mistakes.
        authors suggest that when (in reaction time tasks) errors are committed, the MDS conveys a signal to the frontal cortex where it disinhibits apical dendrites of motor neurons, leading to the ERN.
        MDS inhibits ACC following positive/rewarding events, and disinhibits it following unpredicted errors.
        ERN fires as soon as consequences are detected to be worse than predicted.}
}

