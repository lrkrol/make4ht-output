@article{pope1995biocybernetic,
    title = "Biocybernetic system evaluates indices of operator engagement in automated task",
    author = "Pope, Alan T. and Bogart, Edward H. and Bartolome, Debbie S.",
    year = "1995",
    journal = "Biological Psychology",
    volume = "40",
    number = "1",
    pages = "187--195",
    issn = "0301-0511",
    doi = "10.1016/0301-0511(95)05116-3",
    summary = {a closed-loop method was evaluated to promote mental engagement in automated environment. the goal was to optimise human/system task allocation based on an EEG-based engagement index. the authors describe this as a biocybernetic loop adjusting the level of automation based on brain activity. when engagement is waning, additional tasks are given to the human; when engagement is rising, additional tasks are automated (because engagement indicates that the operator is capable of attentively monitoring the automation). they argue that such a closed-loop feedback system is in an optimal mode of operation when a stable oscillation is reached (reflecting stable engagement). thus, by comparing negative and positive feedback loops driven by different EEG- and EMG-based indices, they identify which index delivers the most stable result in the negative feedback case. beta/(alpha+theta) power best reflected task engagement.}
}

