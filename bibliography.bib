@article{hajcak2006binaryfrn,
    title = "The feedback-related negativity reflects the binary evaluation of good versus bad outcomes",
    author = "Hajcak, Greg and Moser, Jason S. and Holroy,d Clay B. and Simons, Robert F.",
    year = "2006",
    journal = "Biological Psychology",
    volume = "71",
    number = "2",
    pages = "148--154",
    issn = "0301-0511",
    doi = "10.1016/j.biopsycho.2005.04.001",
    summary = {
        experiment 1: simple four-choice gambling task: +25, +5, -5, or -25 ct reward.
        result: larger FRN for losses compared to gains, but no sensitivity to magnitude.
        experiment 2: as before, but with an additional fifth neutral outcome 0 ct.
        result: FRN also observed following neutral (non-positive) outcome.
        FRN appears to be binary: either good, or not.}
}

